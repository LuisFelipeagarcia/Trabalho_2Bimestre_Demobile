<configuration>
  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None"/>
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.7.2"/>
    <httpRuntime targetFramework="4.7.2"/>
    <!-- Área de controle de autenticação-->
    <authentication mode="Forms">
      <forms defaultUrl="~/usu_listar.aspx" loginUrl="~/Default.aspx"></forms>
    </authentication>
    <authorization>
      <deny users="?"/>
    </authorization>
  </system.web>
  

</configuration>
